{
  // https://qiita.com/dtakkiy/items/056867930e66f3489211#%E3%82%AF%E3%83%A9%E3%82%A4%E3%82%A2%E3%83%B3%E3%83%88%E3%82%B5%E3%82%A4%E3%83%89%E3%81%AE%E3%83%87%E3%83%90%E3%83%83%E3%82%AC%E3%81%AE%E8%B5%B7%E5%8B%95
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debugging on Server-Side",
      "type": "node",
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "port": 9230
    },
    {
      "name": "Debugging on Client-Side",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack://_N_E/*": "${webRoot}/*"
      }
    }
  ]
}
